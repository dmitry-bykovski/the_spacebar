{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "ext-json": "*",
        "antishov/doctrine-extensions-bundle": "^1.4",
        "doctrine/annotations": "^1.12",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.8",
        "knplabs/knp-markdown-bundle": "^1.9",
        "knplabs/knp-time-bundle": "^1.16",
        "nexylan/slack-bundle": "2.2.0",
        "php-http/guzzle6-adapter": "1.1.1",
        "php-http/httplug-bundle": "^1.19",
        "sensio/framework-extra-bundle": "^6.1",
        "sensiolabs/security-checker": "^6.0",
        "sonata-project/admin-bundle": "^3.79",
        "sonata-project/doctrine-orm-admin-bundle": "^3.24",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "4.4.17",
        "symfony/console": "4.4.17",
        "symfony/debug-bundle": "4.4.17",
        "symfony/flex": "1.9.9",
        "symfony/framework-bundle": "4.4.17",
        "symfony/google-mailer": "4.4.17",
        "symfony/http-client": "4.4.17",
        "symfony/lts": "^4@dev",
        "symfony/mailer": "4.4.17",
        "symfony/monolog-bundle": "^3.0",
        "symfony/stopwatch": "4.4.17",
        "symfony/translation": "4.4.17",
        "symfony/twig-bundle": "4.4.17",
        "symfony/var-dumper": "4.4.17",
        "symfony/web-profiler-bundle": "4.4.17",
        "symfony/web-server-bundle": "4.4.17",
        "symfony/yaml": "4.4.17",
        "twig/extra-bundle": "^3.3"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "fzaninotto/faker": "^1.9",
        "symfony/dotenv": "4.4.17",
        "symfony/maker-bundle": "^1.30"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {}
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C1TW989CK77ZA7B2H4HC9WAG",
            "allow-contrib": false,
            "require": "4.4.17"
        }
    }
}
